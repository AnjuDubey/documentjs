{{#isConstructor}}
<ul class="search-container">
	{{#children}}
	{{#if children.length}}
	<li class="heading search-container">
		<span>{{type}}</span>
		{{> submenu.mustache}}
	</li>
	{{/if}}
	{{/children}}
</ul>
{{/isConstructor}}

{{^isConstructor}}
<ul class="submenu">
	<li>{{> submenu.mustache}}</li>
</ul>
{{/isConstructor}}
